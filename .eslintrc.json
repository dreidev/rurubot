{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "dreidev"],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "require-jsdoc": 0,
    "no-console": 0,
    "max-len": [2, {
      "code": 120,
      "tabWidth": 2,
      "ignoreUrls": true
    }]
  }
}
